{
  "newfloat{}{}{}[]": {
    "command": "newfloat{type}{placement}{ext}[within]",
    "snippet": "newfloat{${1:type}}{${2:placement}}{${3:ext}}[${4:within}]"
  },
  "floatstyle": {
    "command": "floatstyle",
    "snippet": "floatstyle{${1:defaultstyle}}"
  },
  "floatname": {
    "command": "floatname",
    "snippet": "floatname{${1:type}}{${2:name}}"
  },
  "floatplacement": {
    "command": "floatplacement",
    "snippet": "floatplacement{${1:type}}{${2:default placement specifier}}"
  },
  "restylefloat": {
    "command": "restylefloat",
    "snippet": "restylefloat{figure or table"
  },
  "listof": {
    "command": "listof",
    "snippet": "listof{${1:type}}{${2: title}}"
  }
}