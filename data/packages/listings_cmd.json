{
  "lstalias": {
    "command": "lstalias",
    "snippet": "lstalias{${1:alias}}{${2:language}}"
  },
  "lstdefinestyle": {
    "command": "lstdefinestyle",
    "snippet": "lstdefinestyle{${1:style name}}{${2:kv-list}}"
  },
  "lstinline": {
    "command": "lstinline",
    "snippet": "lstinline"
  },
  "lstinline[]": {
    "command": "lstinline[kv-list]",
    "snippet": "lstinline[${1:kv-list}]"
  },
  "lstinputlisting": {
    "command": "lstinputlisting",
    "snippet": "lstinputlisting{${1:filename}}"
  },
  "lstinputlisting[]{}": {
    "command": "lstinputlisting[kv-list]{filename}",
    "snippet": "lstinputlisting[${2:kv-list}]{${1:filename}}"
  },
  "lstlistingname": {
    "command": "lstlistingname",
    "snippet": "lstlistingname"
  },
  "lstlistlistingname": {
    "command": "lstlistlistingname",
    "snippet": "lstlistlistingname"
  },
  "lstlistoflistings": {
    "command": "lstlistoflistings",
    "snippet": "lstlistoflistings"
  },
  "lstname": {
    "command": "lstname",
    "snippet": "lstname"
  },
  "lstnewenvironment{}[][]{}{}": {
    "command": "lstnewenvironment{name}[params][default]{starting code}{ending code}",
    "snippet": "lstnewenvironment{${1:name}}[${2:params}][${3:default}]{${4:starting code}}{${5:ending code}}"
  },
  "lstnewenvironment{}[]{}{}": {
    "command": "lstnewenvironment{name}[params]{starting code}{ending code}",
    "snippet": "lstnewenvironment{${1:name}}[${2:params}]{${3:starting code}}{${4:ending code}}"
  },
  "lstnewenvironment": {
    "command": "lstnewenvironment",
    "snippet": "lstnewenvironment{${1:name}}{${2:starting code}}{${3:ending code}}"
  },
  "lstset": {
    "command": "lstset",
    "snippet": "lstset{${1:kv-list}}"
  },
  "thelstlisting": {
    "command": "thelstlisting",
    "snippet": "thelstlisting"
  }
}