{
  "tabulinestyle": {
    "command": "tabulinestyle",
    "snippet": "tabulinestyle{${1:style}}"
  },
  "usetabu": {
    "command": "usetabu",
    "snippet": "usetabu{${1:username}}"
  },
  "tabucline": {
    "command": "tabucline",
    "snippet": "tabucline{${1:%<from%>-%<to%>}}"
  },
  "tabucline[]{}": {
    "command": "tabucline[%<spec%>]{%<from%>-%<to%>}",
    "snippet": "tabucline[${2:%<spec%>}]{${1:%<from%>-%<to%>}}"
  },
  "savetabu": {
    "command": "savetabu",
    "snippet": "savetabu{${1:username}}"
  },
  "preamble": {
    "command": "preamble",
    "snippet": "preamble"
  },
  "tabuphantomline": {
    "command": "tabuphantomline",
    "snippet": "tabuphantomline"
  },
  "tabulinesep = %<dimen%>": {
    "command": "tabulinesep = %<dimen%>",
    "snippet": "tabulinesep = %<dimen%>"
  },
  "abovetabulinesep": {
    "command": "abovetabulinesep",
    "snippet": "abovetabulinesep"
  },
  "belowtabulinesep": {
    "command": "belowtabulinesep",
    "snippet": "belowtabulinesep"
  },
  "extrarowsep =%<dimen%>": {
    "command": "extrarowsep =%<dimen%>",
    "snippet": "extrarowsep =%<dimen%>"
  },
  "extrarowsep =^%<dimen%>": {
    "command": "extrarowsep =^%<dimen%>",
    "snippet": "extrarowsep =^%<dimen%>"
  },
  "extrarowsep =_%<dimen%>": {
    "command": "extrarowsep =_%<dimen%>",
    "snippet": "extrarowsep =_%<dimen%>"
  },
  "extrarowsep =_%<dimen%>^%<dimen%>": {
    "command": "extrarowsep =_%<dimen%>^%<dimen%>",
    "snippet": "extrarowsep =_%<dimen%>^%<dimen%>"
  },
  "taburulecolor": {
    "command": "taburulecolor",
    "snippet": "taburulecolor{${1:color}}"
  },
  "taburulecolor|doubleRuleSepColor|": {
    "command": "taburulecolor|doubleRuleSepColor|",
    "snippet": "taburulecolor|doubleRuleSepColor|{${1:color}}"
  },
  "tabureset": {
    "command": "tabureset",
    "snippet": "tabureset"
  },
  "newtabulinestyle": {
    "command": "newtabulinestyle",
    "snippet": "newtabulinestyle{${1:style}}"
  },
  "everyrow": {
    "command": "everyrow",
    "snippet": "everyrow{${1:code}}"
  },
  "taburowcolors [] %<number%> {}": {
    "command": "taburowcolors [<%first line%>] %<number%> {%<first%> .. %<last%>}",
    "snippet": "taburowcolors [${2:<%first line%>}] %<number%> {${1:%<first%> .. %<last%>}}"
  },
  "taburowcolors %<number%> ": {
    "command": "taburowcolors %<number%> ",
    "snippet": "taburowcolors %<number%> {${1:%<first%> .. %<last%>}}"
  },
  "rowfont[]{}": {
    "command": "rowfont[alignment]{fontSpec}",
    "snippet": "rowfont[${2:alignment}]{${1:fontSpec}}"
  },
  "tabudecimal": {
    "command": "tabudecimal",
    "snippet": "tabudecimal{${1:code}}"
  }
}