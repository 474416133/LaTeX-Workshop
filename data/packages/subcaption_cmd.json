{
  "p": {
    "command": "p",
    "snippet": "p"
  },
  "newtheorem": {
    "command": "newtheorem",
    "snippet": "newtheorem{${1:subfigure}}"
  },
  "captionsetup[]{}": {
    "command": "captionsetup[sub]{options}",
    "snippet": "captionsetup[${2:sub}]{${1:options}}"
  },
  "phantomsubcaption": {
    "command": "phantomsubcaption",
    "snippet": "phantomsubcaption"
  },
  "subcaptionbox": {
    "command": "subcaptionbox",
    "snippet": "subcaptionbox"
  },
  "subcaption": {
    "command": "subcaption",
    "snippet": "subcaption{${1:heading}}"
  },
  "subcaption[]{}": {
    "command": "subcaption[list entry]{heading}",
    "snippet": "subcaption[${2:list entry}]{${1:heading}}"
  },
  "subcaption*": {
    "command": "subcaption*",
    "snippet": "subcaption*{${1:heading}}"
  }
}