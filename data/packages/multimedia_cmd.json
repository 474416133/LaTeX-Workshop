    "command": "movie[options]{poster text}{file}",
    "snippet": "movie[${3:options}]{${1:poster text}}{${2:file}}"
  },
  "movie": {
    "command": "movie",
    "snippet": "movie{${1:poster text}}{${2:file}}"
