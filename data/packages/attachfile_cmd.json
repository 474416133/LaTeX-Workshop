{
  "attachfile[]{}": {
    "command": "attachfile[options]{filename}",
    "snippet": "attachfile[${2:options}]{${1:filename}}"
  },
  "attachfile": {
    "command": "attachfile",
    "snippet": "attachfile{${1:filename}}"
  },
  "attachfilesetup": {
    "command": "attachfilesetup",
    "snippet": "attachfilesetup{${1:options}}"
  },
  "noattachfile[]": {
    "command": "noattachfile[options]",
    "snippet": "noattachfile[${1:options}]"
  },
  "notextattachfile[]{}": {
    "command": "notextattachfile[options]{text}",
    "snippet": "notextattachfile[${2:options}]{${1:text}}"
  },
  "notextattachfile": {
    "command": "notextattachfile",
    "snippet": "notextattachfile{${1:text}}"
  },
  "textattachfile[]{}{}": {
    "command": "textattachfile[options]{filename}{text}",
    "snippet": "textattachfile[${3:options}]{${1:filename}}{${2:text}}"
  },
  "textattachfile": {
    "command": "textattachfile",
    "snippet": "textattachfile{${1:filename}}{${2:text}}"
  }
}