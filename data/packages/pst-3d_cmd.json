{
  "psshadow": {
    "command": "psshadow",
    "snippet": "psshadow{${1}}"
  },
  "psshadow*": {
    "command": "psshadow*",
    "snippet": "psshadow*{${1}}"
  },
  "pstilt": {
    "command": "pstilt",
    "snippet": "pstilt{${1}}{${2}}"
  },
  "psTilt": {
    "command": "psTilt",
    "snippet": "psTilt{${1}}{${2}}"
  },
  "ThreeDput": {
    "command": "ThreeDput",
    "snippet": "ThreeDput{${1}}"
  },
  "ThreeDput(x0,y0,z0)": {
    "command": "ThreeDput(x0,y0,z0)",
    "snippet": "ThreeDput(x0,y0,z0){${1}}"
  }
}