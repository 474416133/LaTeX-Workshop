{
  "todo": {
    "command": "todo",
    "snippet": "todo{${1:text%todo}}"
  },
  "todo[]{}": {
    "command": "todo[options]{text%todo}",
    "snippet": "todo[${2:options}]{${1:text%todo}}"
  },
  "listoftodos": {
    "command": "listoftodos",
    "snippet": "listoftodos"
  },
  "listoftodos[]": {
    "command": "listoftodos[caption]",
    "snippet": "listoftodos[${1:caption}]"
  },
  "missingfigure": {
    "command": "missingfigure",
    "snippet": "missingfigure{${1:text%todo}}"
  },
  "missingfigure[]{}": {
    "command": "missingfigure[options]{text%todo}",
    "snippet": "missingfigure[${2:options}]{${1:text%todo}}"
  }
}